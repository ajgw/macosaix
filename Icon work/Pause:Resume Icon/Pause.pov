global_settings { assumed_gamma 2.2 }#include "colors.inc"#include "textures.inc"#include "glass.inc"background { color White }#declare Glass = texture { pigment { color Clear } finish { F_Glass6 } }camera{  location <0, 22, 0>  right <4/4, 0, 0>  up <0, 1, 0>  sky <0, 1, 0>  direction <0, 0, 2.21>  look_at <0, 0, 0>}light_source{	<100, 200, 25>	colour White	area_light	<20, 0, 0>, <0, 0, 20>, 8, 8}box{	<0, 0, -.001>, <1, 1, .001>	pigment { image_map { jpeg "Flower.jpg" } }	translate <-.5, -.5, 0>	scale <8, 6, 1>	rotate 90*x	translate (0.5 - clock) * 10 * x	finish	{		ambient .4		diffuse .6	}}union{	// Lens	intersection	{		sphere		{			<0, 2.5, 0> 3			material { texture { T_Glass3 } interior { I_Glass }    }		}		sphere		{			<0, 1, 0> 3			material { texture { T_Glass3 } interior { I_Glass }    }		}	}	// Lens frame	difference	{		cylinder		{			<0, 2.25, 0>, <0, 2.75, 0>, 3			texture { Bright_Bronze }		}		cylinder		{			<0, 2, 0>, <0, 3, 0>, 2.8		}	}	// Handle	cylinder	{		<1.75, 0, -1.75>, <8, 0, -8>, .75		texture {			pigment {				DMFWood4 // pre-defined in textures.inc				scale 4 // scale by the same amount in all				rotate y*-40			}		}	}}